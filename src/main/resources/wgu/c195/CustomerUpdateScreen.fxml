<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="555.0" prefWidth="561.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerUpdateController">
    <children>
        <TextField fx:id="customerIDField" disable="true" layoutX="44.0" layoutY="116.0" promptText="Customer ID" text="Customer ID" />
        <TextField fx:id="customerNameField" layoutX="44.0" layoutY="180.0" promptText="Name" />
        <TextField fx:id="addressField" layoutX="44.0" layoutY="247.0" prefHeight="55.0" prefWidth="224.0" promptText="Address" />
        <TextField fx:id="postalCodeField" layoutX="44.0" layoutY="330.0" promptText="Postal Code" />
        <TextField fx:id="phoneField" layoutX="44.0" layoutY="400.0" promptText="Phone Number" />
        <ComboBox fx:id="countryComboBox" layoutX="294.0" layoutY="116.0" onAction="#onSelectCountry" prefHeight="39.0" prefWidth="224.0" promptText="Select a country..." />
        <ComboBox fx:id="divisionComboBox" layoutX="294.0" layoutY="180.0" prefHeight="39.0" prefWidth="224.0" promptText="Select a division..." />
        <Button layoutX="436.0" layoutY="470.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
        <Button layoutX="341.0" layoutY="470.0" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save" />
        <Label layoutX="108.0" layoutY="29.0" text="Update Customer">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
