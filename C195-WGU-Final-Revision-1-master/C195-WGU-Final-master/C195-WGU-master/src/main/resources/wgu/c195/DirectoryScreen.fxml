<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="355.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DirectoryController">
   <children>
      <Label layoutX="75.0" layoutY="50.0" text="MAIN DIRECTORY">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <GridPane layoutX="85.0" layoutY="140.0" prefHeight="159.0" prefWidth="185.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button alignment="CENTER" contentDisplay="CENTER" layoutX="18.0" layoutY="8.0" mnemonicParsing="false" onAction="#onCustomersButtonClick" prefHeight="35.0" prefWidth="149.0" text="Customers">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
            <Pane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <Button alignment="CENTER" contentDisplay="CENTER" layoutX="18.0" layoutY="8.0" mnemonicParsing="false" onAction="#onAppointmentsButtonClick" prefHeight="39.0" prefWidth="149.0" text="Appointments">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
            <Pane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
               <children>
                  <Button alignment="CENTER" contentDisplay="CENTER" layoutX="18.0" layoutY="8.0" mnemonicParsing="false" onAction="#onReportsButtonClick" prefHeight="35.0" prefWidth="149.0" text="Reports">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
         </children>
      </GridPane>
      <Button layoutX="285.0" layoutY="345.0" mnemonicParsing="false" onAction="#onExitButtonClick" text="Exit" />
   </children>
</AnchorPane>
